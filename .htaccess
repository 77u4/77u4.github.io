RewriteEngine On

RewriteRule ^contact[^/]$ contact.html
RewriteRule ^kontakt[^/]$ contact.html
RewriteRule ^theme[^/]$ theme.html
RewriteRule ^impressum[^/]$ impressum.html

RewriteCond %{HTTPS} !=on
RewriteCond %{ENV:HTTPS} !=on
RewriteRule .* https://%{SERVER_NAME}%{REQUEST_URI} [R=301,L]
AddOutputFilterByType DEFLATE text/html text/plain text/xml text/css application/javascript text/javascript
Header set Life "1 falsches"
Header set Access-Control-Allow-Origin "*"
